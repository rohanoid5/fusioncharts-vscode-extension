{
  "name": "fusioncharts-code-generator",
  "displayName": "FusionCharts Code Generator",
  "description": "A Visual Studio Code extension to generate code to simplify implementation of FusionCharts.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.31.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rohanoid5/fusioncharts-vscode-extension"
  },
  "icon": "logo.png",
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippet-js.json"
      },
      {
        "language": "html",
        "path": "./snippets/snippet-html.json"
      }
    ]
  },
  "publisher": "rohanoid5",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
